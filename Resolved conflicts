[1mdiff --git a/server.js b/server.js[m
[1mnew file mode 100644[m
[1mindex 0000000..4b57e82[m
[1m--- /dev/null[m
[1m+++ b/server.js[m
[36m@@ -0,0 +1,54 @@[m
[32m+[m[32mconst path = require("path");[m
[32m+[m
[32m+[m[32m// Require the fastify framework and instantiate it[m
[32m+[m[32mconst fastify = require("fastify")({[m
[32m+[m[32m  // set this to true for detailed logging:[m
[32m+[m[32m  logger: false,[m
[32m+[m[32m});[m
[32m+[m
[32m+[m[32m// Setup our static files[m
[32m+[m[32mfastify.register(require("@fastify/static"), {[m
[32m+[m[32m  root: path.join(__dirname, "public"),[m
[32m+[m[32m  prefix: "/", // optional: default '/'[m
[32m+[m[32m});[m
[32m+[m
[32m+[m[32m// fastify-formbody lets us parse incoming forms[m
[32m+[m[32mfastify.register(require("@fastify/formbody"));[m
[32m+[m
[32m+[m[32m// point-of-view is a templating manager for fastify[m
[32m+[m[32mfastify.register(require("@fastify/view"), {[m
[32m+[m[32m  engine: {[m
[32m+[m[32m    handlebars: require("handlebars"),[m
[32m+[m[32m  },[m
[32m+[m[32m});[m
[32m+[m
[32m+[m[32m// Our main GET home page route, pulls from src/pages/index.hbs[m
[32m+[m[32mfastify.get("/", function (request, reply) {[m
[32m+[m[32m  // params is an object we'll pass to our handlebars template[m
[32m+[m[32m  let params = {[m
[32m+[m[32m    greeting: "Hello Node!",[m
[32m+[m[32m  };[m
[32m+[m[32m  // request.query.paramName <-- a querystring example[m
[32m+[m[32m  return reply.view("/src/pages/index.hbs", params);[m
[32m+[m[32m});[m
[32m+[m
[32m+[m[32m// A POST route to handle form submissions[m
[32m+[m[32mfastify.post("/", function (request, reply) {[m
[32m+[m[32m  let params = {[m
[32m+[m[32m    greeting: "Hello Form!",[m
[32m+[m[32m  };[m
[32m+[m[32m  // request.body.paramName <-- a form post example[m
[32m+[m[32m  return reply.view("/src/pages/index.hbs", params);[m
[32m+[m[32m});[m
[32m+[m
[32m+[m[32m// Run the server and report out to the logs[m
[32m+[m[32mfastify.listen([m
[32m+[m[32m  { port: process.env.PORT, host: "0.0.0.0" },[m
[32m+[m[32m  function (err, address) {[m
[32m+[m[32m    if (err) {[m
[32m+[m[32m      console.error(err);[m
[32m+[m[32m      process.exit(1);[m
[32m+[m[32m    }[m
[32m+[m[32m    console.log(`Your app is listening on ${address}`);[m
[32m+[m[32m  }[m
[32m+[m[32m);[m
