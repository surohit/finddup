[36m@@ -0,0 +1,25 @@[m
[32m+[m[32mconst express = require('express');[m
[32m+[m[32mconst multer = require('multer');[m
[32m+[m[32mconst fs = require('fs');[m
[32m+[m[32mconst app = express();[m
[32m+[m
[32m+[m[32mconst upload = multer();[m
[32m+[m
[32m+[m[32mapp.post('/compare', upload.none(), (req, res) => {[m
[32m+[m[32m    const folder1 = req.body.folder1;[m
[32m+[m[32m    const folder2 = req.body.folder2;[m
[32m+[m
[32m+[m[32m    // Perform comparison logic based on folder paths here[m
[32m+[m[32m    // For now, we assume we find some duplicate files[m
[32m+[m[32m    const duplicateFiles = ['file1.txt', 'file2.txt']; // Example[m
[32m+[m
[32m+[m[32m    // Return the comparison result as CSV[m
[32m+[m[32m    const csvContent = 'File Name\n' + duplicateFiles.join('\n');[m
[32m+[m[32m    res.setHeader('Content-Type', 'text/csv');[m
[32m+[m[32m    res.setHeader('Content-Disposition', 'attachment; filename="comparison_result.csv"');[m
[32m+[m[32m    res.send(csvContent);[m
[32m+[m[32m});[m
[32m+[m
[32m+[m[32mapp.listen(3000, () => {[m
[32m+[m[32m    console.log('Server started on port 3000');[m
[32m+[m[32m});[m
